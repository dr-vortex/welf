{
	"env": {
		"es2022": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": ["eslint:recommended"],
	"rules": {
		"no-useless-escape": "warn",
		"no-unused-vars": "warn",
		"no-mixed-spaces-and-tabs": "warn",
		"no-unreachable": "warn",
		"no-extra-semi": "warn",
		"no-fallthrough": "off",
		"no-empty": "warn",
		"no-case-declarations": "off"
	}
}
